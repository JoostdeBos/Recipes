<% title 'Gebruikers' %>
<%= form_tag users_path, :method => :get, :id => "users_search", :autocomplete => 'off' do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", :name => nil, :class => 'btn primary' %>
  </p>
<% end %>

<div id="users-index"><%= render 'users' %></div>